<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ziro.mapper.UserMapper">
    <select id="findByName" parameterType="string" resultType="WeiXiuEntity">
        select * from table1 where name like #{name} and type = #{type};
    </select>
    <insert id="addData" parameterType="WeiXiuEntity">
        insert into table1(name,type,yuanyin,cuoshi,houguo) values (#{name},#{type},#{yuanyin},#{cuoshi},#{houguo})
    </insert>
    <select id="findAll" resultType="WeiXiuEntity">
         select * from table1
    </select>
    <select id="findByType" resultType="WeiXiuEntity">
         select * from table1 where type = #{type};
    </select>

</mapper>